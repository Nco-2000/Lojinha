<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% if (success == false) { %>
        <h1>No categories exist</h1>
    <% } else{ %>
        <% categories.forEach(cat => { %>
            <%= cat.Name %>
            <%= cat.Qtd_items %>
            <%= cat.image_id %>
            <%= cat.ID_Category %>
            <a href="/Categories/<%= cat.ID_Category %>">Edit</a>
            <br>
        <% }) %> 
    <% } %>    
</body>
</html>